{
  "name": "@ts-graphql/rosetta-codegen",
  "version": "0.0.0",
  "main": "lib/index.js",
  "repository": "https://github.com/ts-graphql/rosetta-codegen",
  "author": "stephentuso",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "typescript": "^3.3.4000"
  },
  "peerDependencies": {
    "graphql": "^14.1.1"
  },
  "devDependencies": {
    "@ts-graphql/rosetta": "^0.1.5",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.6",
    "coveralls": "^3.0.3",
    "graphql": "^14.1.1",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/*.d.ts",
      "!**/__tests__/**"
    ]
  }
}
